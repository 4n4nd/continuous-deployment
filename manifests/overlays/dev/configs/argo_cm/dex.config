connectors:
  # OpenShift
  - type: openshift
    id: openshift
    name: OpenShift
    config:
      issuer: https://kubernetes.default.svc
      clientID: system:serviceaccount:aicoe-argocd-dev:argocd-dex-server
      clientSecret: $dex.serviceaccount.clientSecret
      insecureCA: true
      groups:
        - dev-group